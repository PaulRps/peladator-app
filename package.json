{
  "name": "peladator-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy-conf.json -o",
    "build": "ng build",
    "postbuild": "npm run deploy",
    "predeploy": "rimraf ../peladator-api/src/main/resources/static/ && mkdirp ../peladator-api/src/main/resources/static/",
    "deploy": "copyfiles -f dist/** ../peladator-api/src/main/resources/static",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prettier-format": "prettier --write --config ./.prettierrc.yml ./*.{ts,css,sass,html,json}",
    "prettier-check": "prettier -c --debug-check --config ./.prettierrc.yml ./*.{ts,css,sass,html,json}",
    "compile-themes": "node-sass src/assets/styles/themes/source --output src/assets/styles/themes/compiled --output-style compressed",
    "tailwind-style": "tailwindcss build src/assets/styles/tailwindcss/tailwind.css -o src/assets/styles/tailwindcss/compiled-style.css"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.2",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^9.0.2",
    "@angular/compiler": "^9.0.2",
    "@angular/core": "~9.0.2",
    "@angular/forms": "~9.0.2",
    "@angular/localize": "^9.0.2",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~9.0.2",
    "@angular/platform-browser-dynamic": "~9.0.2",
    "@angular/router": "~9.0.2",
    "@ng-bootstrap/ng-bootstrap": "^4.2.2",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "moment": "^2.24.0",
    "ngx-mask": "^8.1.7",
    "popper": "^1.0.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.3",
    "@angular/cli": "~9.0.3",
    "@angular/compiler-cli": "^9.0.2",
    "@angular/language-service": "~9.0.2",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "copyfiles": "^2.1.0",
    "husky": "^4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.0",
    "rimraf": "^2.6.3",
    "tailwindcss": "^1.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "3.7.5"
  }
}
